#!/bin/sh

start()
{
  local self="`realpath "$0"`"
  local base="`dirname "$self"`"

  source "$base/../env" &&
  merge_environment /srv/software &&
  \
  exec nohup "`which sshd`" -D \
    >> '/srv/storage/medic-core/openssh/logs/startup.log' 2>&1
}

main()
{
  start
  return "$?"
}

main
exit "$?"

